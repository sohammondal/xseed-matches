  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.18.2",
    "express-validator": "^6.2.0",
    "mongoose": "^5.7.12"
  },
  "devDependencies": {
    "concurrently": "^5.0.0",
    "jest": "^24.9.0",
    "nodemon": "^2.0.1",
    "supertest": "^4.0.2"
  }
}